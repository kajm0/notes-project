{
  "expo": {
    "name": "Notes Mobile",
    "slug": "notes-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "notes",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.notes.app"
    },
    "android": {
      "package": "com.notes.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}


